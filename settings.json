  "screencastMode.mouseIndicatorSize": 40,
  // "workbench.startupEditor": "terminal",
  "breadcrumbs.enabled": false,
  "[javascript]": { "editor.defaultFormatter": "esbenp.prettier-vscode" },
  "debug.console.fontSize": 18,
  "terminal.integrated.fontSize": 15,
  "vim.easymotionMarkerBackgroundColor": "#0000::",
  "vim.easymotionDimColor": "blue",
  "vim.commandLineModeKeyBindings": [],
  "vim.leader": "<space>",
  "vim.normalModeKeyBindingsNonRecursive": [
    { "before": ["leader", "h"], "commands": ["vscode-harpoon.editEditors"] },
    { "before": ["<leader>", "H"], "commands": ["vscode-harpoon.addEditor"] },

    {
      "before": ["<leader>", "f", "h"],
      "commands": ["vscode-harpoon.editorQuickPick"]
    },
    {
      "before": ["<leader>", "f", "h"]
    },

    { "before": ["<leader>", "d", "m"], "commands": [":delmarks!"] },

    {
      "before": ["<leader>", "b", "m"],
      "commands": ["bookmarks.toggle"]
    },

    {
      "before": ["leader", "b", "c"],
      "commands": ["bookmarks.clear"]
    },
    {
      "before": ["leader", "b", "l"],
      "commands": ["bookmarks.list"]
    },

    // FILE Tree
    {
      "before": ["<leader>", "z"],
      "commands": [
        {
          "command": "workbench.action.toggleZenMode"
        }
      ]
    },
    {
      "before": ["<leader>", "l", "s"],
      "commands": [
        {
          "command": "workbench.action.showEditorsInActiveGroup"
        }
      ]
    },
    {
      "before": ["<leader>", "l", "s"],
      "commands": [
        {
          "command": "workbench.action.showEditorsInActiveGroup"
        }
      ]
    },
    {
      "before": ["<leader>", "f", "f"],
      "commands": [
        {
          "command": "workbench.action.quickOpen"
        }
      ]
    },

    {
      "before": ["<leader>", "f", "m"],
      "commands": [
        {
          "command": ":marks"
        }
      ]
    },
    {
      "before": ["<leader>", "f", "t"],
      "after": [],
      "commands": [
        {
          "command": "workbench.action.gotoSymbol",
          "args": {
            "prefix": "@"
          }
        }
      ]
    },

    {
      "before": ["<leader>", "q"],
      "after": [],
      "commands": [
        {
          "command": ":q",
          "args": []
        }
      ]
    },
    {
      "before": ["<leader>", "w", "v"],
      "after": [],
      "commands": [
        {
          "command": ":split",
          "args": []
        }
      ]
    },

    {
      "before": ["<leader>", "w", "s"],
      "after": [],
      "commands": [
        {
          "command": ":vs",
          "args": []
        }
      ]
    }
  ],
  "vim.normalModeKeyBindings": [
    {
      "before": ["<leader>", "e"],
      "commands": ["workbench.view.explorer"]
    },
    {
      "before": ["g", "p", "d"],
      "commands": ["editor.action.peekDefinition"]
    },
    {
      "before": ["g", "h"],
      "commands": ["editor.action.showDefinitionPreviewHover"]
    },
    {
      "before": ["g", "i"],
      "commands": ["editor.action.goToImplementation"]
    },
    {
      "before": ["g", "p", "i"],
      "commands": ["editor.action.peekImplementation"]
    },
    {
      "before": ["g", "q"],
      "commands": ["editor.action.quickFix"]
    },
    {
      "before": ["g", "r"],
      "commands": ["editor.action.referenceSearch.trigger"]
    },
    {
      "before": ["g", "t"],
      "commands": ["editor.action.goToTypeDefinition"]
    },
    {
      "before": ["g", "p", "t"],
      "commands": ["editor.action.peekTypeDefinition"]
    },
    {
      "before": ["<leader>", "f", "g"],
      "commands": [
        {
          "command": "quickgrep.action",
          "title": "Quick Grep"
        }
      ]
    },

    // {
    //   "before": ["<leader>", "f"],
    //   "commands": ["revealInExplorer"]
    // },
    {
      "before": ["ctrl+u"],
      "after": ["ctrl+u", "z", "z"]
    },

    {
      "before": ["ctrl+d"],
      "after": ["ctrl+d", "z", "z"]
    },

    // easy-motion
    {
      "before": ["J"],
      "after": ["<leader>", "<leader>", "w"]
    },

    {
      "before": ["K"],
      "after": ["<leader>", "<leader>", "b"]
    },
    // NAVIGATION
    // switch between buffers
    { "before": ["<S-h>"], "commands": ["workbench.action.previousEditor"] },
    { "before": ["<S-l>"], "commands": ["workbench.action.nextEditor"] }
  ],
  "vim.easymotionKeys": "hklyuionmqwertzxcvbasdgjf",
